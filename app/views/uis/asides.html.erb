
<div class="padding">
	<h5 class="_300 margin">Aside style</h5>
	<div class="row">
		<div class="col-md-6 col-lg-3">
			<nav class="navside white r box-shadow-z0 m-b">
				<%= render "blocks/aside_0" %>
			</nav>
		</div>
		<div class="col-md-6 col-lg-3">
			<div class="navside white r box-shadow-z0 m-b">
				<%= render "blocks/aside_1" %>
			</div>
		</div>
		<div class="col-md-6 col-lg-3">
			<div class="navside white r box-shadow-z0 m-b">
				<%= render "blocks/aside_2" %>
			</div>
